        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "spectron": "^15.0.0",
        "ts-jest": "^27.1.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.2",
        "unplugin-auto-import": "^0.5.1",
        "unplugin-vue-components": "^0.17.6",
        "vant": "^3.3.6",
        "vite": "^2.7.1",
        "vite-plugin-style-import": "^1.4.0",
        "vitepress": "^0.20.4",
        "vitest": "^0.1.16",
        "vue": "^3.2.26",
        "vue-eslint-parser": "^8.0.1",
        "vue-i18n": "^9.2.0-beta.23",
        "vue-router": "^4.0.12",
        "vue-tsc": "^0.29.8",
        "vuex": "^4.0.2",
        "webpack": "^5.65.0"
    },
    "nodemonConfig": {
        "watch": [
            "/dist/**/*",
            "/dist/*"
        ],
        "delay": 200
    },
    "verson": "0.0.2-hotpatch1.1"
}